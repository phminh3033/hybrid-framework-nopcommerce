<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="NopCommerce Web">
    <listeners>
        <listener class-name="commons.MethodListener"/>
        <!--<listener class-name="reportConfig.AllureTestListener"/>-->
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>
    <test name="Run on Firefox" preserve-order="true">
        <!--<parameter name="adminUrl" value="https://admin-demo.nopcommerce.com"/>
        <parameter name="endUserUrl" value="https://demo.nopcommerce.com"/>-->
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="com.nopcommerce.account.Level_22_Close_Browser"/>
        </classes>
    </test>
    <!--<test name="Run on Chrome">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.nopcommerce.account.Level_17_ReportNG"/>
        </classes>
    </test>

    <test name="Run on Edge">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="com.nopcommerce.account.Level_16_Log"/>
        </classes>
    </test>-->
</suite>