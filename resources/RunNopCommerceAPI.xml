<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="NopCommerce API">
    <listeners>
        <listener class-name="commons.MethodListener"/>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>
    <test name="Run on Firefox Payment">
        <!--<parameter name="adminUrl" value="https://admin-demo.nopcommerce.com"/>
        <parameter name="endUserUrl" value="https://demo.nopcommerce.com"/>-->
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="com.nopcommerce.account.Level_15_Assert_Verify"/>
        </classes>
    </test>
    <test name="Run on Chrome Order">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.nopcommerce.account.Level_15_Assert_Verify"/>
        </classes>
    </test>
</suite>